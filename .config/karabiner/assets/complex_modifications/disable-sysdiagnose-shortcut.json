{
  "title": "Disable Sysdiagnose Shortcut",
  "rules": [
    {
      "description": "Disable command+control+option+shift+comma",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "comma",
            "modifiers": {
              "mandatory": [
                "command",
                "shift",
                "option",
                "control"
              ]
            }
          },
          "to": {
              "key_code": "slash"
          }
        }
      ]
    }
  ]
}
