{
    "editor.autoIndent": true,
    //"atomKeymap.promptV3Features": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.fontSize": 13,
    "editor.fontFamily": "\"Fira Code Retina\", \"FuraCode NF Retina\", \"FiraCode-Retina\", Roboto Mono for Powerline, Inconsolata-dz, hack",
    // Controls how lines should wrap. Can be:
    //  - 'off' (disable wrapping),
    //  - 'on' (viewport wrapping),
    //  - 'wordWrapColumn' (wrap at `editor.wordWrapColumn`) or
    //  - 'bounded' (wrap at minimum of viewport and `editor.wordWrapColumn`).
    "editor.wordWrap": "on",
    // Controls the wrapping column of the editor when `editor.wordWrap` is 'wordWrapColumn' or 'bounded'.
    "editor.wordWrapColumn": 80,
    // Controls the indentation of wrapped lines. Can be one of 'none', 'same' or 'indent'.
    "editor.wrappingIndent": "indent",
    // Wrap attributes.
    "html.format.wrapAttributes": "auto",
    // Maximum amount of characters per line (0 = disable).
    "html.format.wrapLineLength": 120,
    // Use `*` or `_` to wrap italic text
    "markdown.extension.italic.indicator": "*",
    "explorer.confirmDelete": false,
    "window.zoomLevel": 0,
    "files.exclude": {
        "*.idea": true,
        "**/.DS_Store": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/CVS": true,
        "*venv": true
    },
    "html.format.endWithNewline": true,
    "html.format.indentHandlebars": true,
    "html.format.indentInnerHtml": true,
    "html.format.unformatted": "link, href, class",
    "html.format.contentUnformatted": "pre, code, textarea, rel, type, href",
    "python.formatting.provider": "autopep8",
    "python.formatting.autopep8Args": [
        "--indent-size=4"
    ],
    //"prettier.useTabs": true,
    "editor.renderIndentGuides": true,
    "workbench.statusBar.visible": true,
    "workbench.activityBar.visible": false,
    "explorer.confirmDragAndDrop": false,
    "terminal.integrated.macOptionIsMeta": true,
    "terminal.integrated.rendererType": "canvas",
    "files.associations": {
        "*.rstheme": "css",
        "*.rmd": "markdown",
        "*.inc": "txt"
    },
    "update.enableWindowsBackgroundUpdates": false,
    "update.showReleaseNotes": false,
    //"terminal.enableAppInsights": false,
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "terminal.integrated.shell.windows": "C:\\Program Files\\PowerShell\\6\\pwsh.exe",
    "terminal.integrated.fontSize": 12,
    //"bracketPairColorizer.independentPairColors": [
    //	["()", ["Silver", "Pink", "LightGreen"], "Red"],
    //	["()", ["Silver", "Pink", "LightGreen"], "Red"],
    //	["()", ["Silver", "Pink", "LightGreen"], "Red"]
    //],
    //"bracketPairColorizer.consecutivePairColors": [
    //	"()",
    //	"[]",
    //	"{}",
    //	["Silver", "Pink", "LightGreen"],
    //	"Red"
    //],
    "workbench.settings.openDefaultSettings": true,
    "editor.minimap.enabled": false,
    "alignment.operatorPadding": "left",
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": false
    },
    "editor.quickSuggestionsDelay": 10,
    "editor.parameterHints.enabled": false,
    "python.autoComplete.addBrackets": true,
    "search.location": "panel",
    "terminal.integrated.cursorBlinking": true,
    "python.terminal.activateEnvironment": false,
    "markdown.preview.breaks": true,
    "markdown-preview-enhanced.mermaidTheme": "default",
    "markdown-preview-enhanced.revealjsTheme": "none.css",
    //"emmet.showAbbreviationSuggestions": false,
    //"emmet.showExpandedAbbreviation": "never",
    //"editor.wordBasedSuggestions": false,
    //"markdownlint.run": "onSave",
    "markdown.extension.toc.updateOnSave": false,
    "javascript.suggest.enabled": false,
    "typescript.suggest.enabled": false,
    // Configure editor settings to be overridden for [markdown] language.
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": false,
        "editor.wordWrapColumn": 100
    },
    "[less]": {
        "editor.quickSuggestions": true
    },
    // (Markdown) wrap prose over multiple lines
    //"prettier.proseWrap": "preserve",
    "markdown.preview.scrollEditorWithPreview": false,
    "markdown.preview.scrollPreviewWithEditor": false,
    "markdown-preview-enhanced.automaticallyShowPreviewOfMarkdownBeingEdited": false,
    "markdown-preview-enhanced.scrollSync": false,
    "markdown-preview-enhanced.printBackground": true,
    //"powershell.powerShellExePath": "C:\\Program Files\\PowerShell\\6\\pwsh.exe",
    //"powershell.codeFormatting.pipelineIndentationStyle": "IncreaseIndentationAfterEveryPipeline",
    //"powershell.codeFormatting.useCorrectCasing": true,
    "editor.formatOnType": true,
    "editor.formatOnPaste": true,
    "breadcrumbs.enabled": true,
    "r.lsp.debug": true,
    "javascript.format.insertSpaceAfterSemicolonInForStatements": false,
    "powershell.integratedConsole.showOnStartup": false,
    "editor.codeLens": false,
    "git.autorefresh": false,
    "git.countBadge": "tracked",
    "git.decorations.enabled": false,
    "git.detectSubmodules": false,
    "git.ignoreMissingGitWarning": true,
    "git.openDiffOnClick": false,
    "git.pullTags": false,
    "git.showInlineOpenFileAction": false,
    "git.showProgress": false,
    "git.useForcePushWithLease": false,
    "typescript.suggest.paths": false,
    "terminal.external.windowsExec": "pwsh.exe",
    "python.linter": "flake8",
    "python.globalModuleInstallation": true,
    "gitProjectManager.maxDepthRecursion": 1,
    "projectManager.any.maxDepthRecursion": 2,
    "projectManager.checkInvalidPathsBeforeListing": false,
    "explorer.sortOrder": "filesFirst",
    "code-runner.enableAppInsights": false,
    "workbench.iconTheme": "ayu",
    "editor.copyWithSyntaxHighlighting": false,
    "vsintellicode.typescript.completionsEnabled": false,
    "vsintellicode.java.completionsEnabled": false,
    "code-runner.clearPreviousOutput": true,
    "code-runner.saveFileBeforeRun": true,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "workbench.settings.useSplitJSON": true,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.suggest.localityBonus": true,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.fontLigatures": true,
    "editor.matchBrackets": false,
    "powershell.powerShellExePath": "\"C:\\Users\\RRoach\\scoop\\shims\\pwsh.exe\"",
    "window.restoreWindows": "none",
    "csstriggers.showLegend": true,
    "csstriggers.showExtendedInformation": true,
    "workbench.enableExperiments": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.colorTheme": "Alice",
    "todohighlight.keywords": [
        // "HACK:",
        // "FIXME:",
        // "TODO:",
        // "FIX:",
        "TODO",
        "HACK",
        "FIXME",
        "FIX"
        
    ],
    "todohighlight.defaultStyle": {
        "color": "#000000",
        "backgroundColor": "#8bfca5",
        // "overviewRulerColor": "#8bfca5",
        "overviewRulerLane": "2",
        "isWholeLine": false
    },
    "window.title": "${activeEditorMedium}",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorBlinking": "smooth",
    "editor.dragAndDrop": false,
    "workbench.editor.restoreViewState": false,
    "editor.formatOnSave": true,
    "files.hotExit": "off",
    "files.defaultLanguage": "txt",
    "extensions.ignoreRecommendations": true,
    "python.dataScience.ignoreVscodeTheme": true,
    "python.dataScience.askForKernelRestart": false,
    "python.jediEnabled": false
}