{
    "window.zoomLevel": 0,
    "editor.minimap.enabled": false,
    "editor.cursorStyle": "underline",
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "update.enableWindowsBackgroundUpdates": false,
    "update.showReleaseNotes": false,
    "files.associations": {
        "*.rstheme": "css",
        ".functions": "shellscript",
        ".aliases": "shellscript"
    },
    "css.lint.duplicateProperties": "warning",
    "css.validate": false,
    "editor.wordWrap": "on",
    "editor.accessibilitySupport": "off",
    "editor.codeLens": false,
    "editor.quickSuggestionsDelay": 50,
    "workbench.fontAliasing": "antialiased",
    "workbench.statusBar.feedback.visible": false,
    "html.validate.styles": false,
    "less.lint.duplicateProperties": "warning",
    "scss.lint.duplicateProperties": "warning",
    "emmet.showAbbreviationSuggestions": false,
    "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
    "editor.parameterHints.cycle": true,
    "editor.snippetSuggestions": "bottom",
    "git.autorefresh": false,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.hover.enabled": false,
    "editor.suggest.localityBonus": true,
    "editor.suggest.shareSuggestSelections": true,
    "editor.tabCompletion": "on",
    "python.autoComplete.addBrackets": true,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.fontSize": 13,
    "lua.format.indentCount": 4,
    //"gitlens.views.repositories.location": "gitlens",
    //"gitlens.views.fileHistory.location": "gitlens",
    //"gitlens.views.lineHistory.location": "gitlens",
    //"gitlens.views.compare.location": "gitlens",
    //"gitlens.views.search.location": "gitlens",
    //s"gitlens.currentLine.enabled": false,
    //"gitlens.hovers.currentLine.over": "line",
    //"gitlens.blame.toggleMode": "window",
    //"gitlens.blame.highlight.enabled": false,
    //"gitlens.blame.heatmap.enabled": false,
    "workbench.iconTheme": "material-icon-theme",
    "markdown.extension.print.imgToBase64": true,
    "markdown.extension.toc.githubCompatibility": true,
    "json.format.enable": false,
    "vim.disableExtension": true,
    "gitProjectManager.displayProjectPath": true,
    "gitProjectManager.baseProjectsFolders": [
        "/Users/Riley/.dot",
        "/Users/Riley/Documents/Projects/github"
    ],
    "gitProjectManager.maxDepthRecursion": 1,
    "window.title": "${activeEditorLong}",
    "workbench.activityBar.visible": true,
    "gitlens.showWhatsNewAfterUpgrades": false,
    "workbench.colorTheme": "Alice",
    "editor.fontFamily": "Fira Code, SFMono-Regular, Menlo, Monaco, 'Courier New', monospace",
    "gitlens.advanced.similarityThreshold": 0,
    "gitlens.advanced.telemetry.enabled": false,
    "scm.providers.visible": 0
}