{
    "window.zoomLevel": 0,
    "editor.minimap.enabled": false,
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "update.showReleaseNotes": false,
    "files.associations": {
        "*.rstheme": "css",
        ".functions": "shellscript",
        ".aliases": "shellscript",
        "*.rmd": "markdown",
        "Rpkg": "r"
    },
    "css.lint.duplicateProperties": "warning",
    "editor.wordWrap": "on",
    "editor.codeLens": false,
    "editor.quickSuggestions": {
        "other": true,
        "comments": true,
        "strings": false
    },
    "less.lint.duplicateProperties": "warning",
    "scss.lint.duplicateProperties": "warning",
    "emmet.showAbbreviationSuggestions": false,
    "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
    "editor.parameterHints.cycle": true,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.hover.enabled": false,
    "editor.suggest.localityBonus": true,
    "editor.tabCompletion": "onlySnippets",
    "python.autoComplete.addBrackets": true,
    "editor.fontSize": 13,
    "lua.format.indentCount": 4,
    "markdown.extension.print.imgToBase64": true,
    "markdown.extension.toc.githubCompatibility": true,
    "json.format.enable": false,
    // "update.enableWindowsBackgroundUpdates": false,
    // "workbench.statusBar.feedback.visible": false,
    //"vim.disableExtension": true,
    // "gitProjectManager.displayProjectPath": true,
    // "gitProjectManager.baseProjectsFolders": [
        // "/Users/Riley/.dot",
        // "/Users/Riley/Documents/Projects/github"
    // ],
    // "gitProjectManager.maxDepthRecursion": 1,
    // "gitProjectManager.checkRemoteOrigin": false,
    // "gitProjectManager.searchInsideProjects": false,
    //"gitlens.showWhatsNewAfterUpgrades": false,
    //"gitlens.advanced.similarityThreshold": 0,
    //"gitlens.advanced.telemetry.enabled": false,
    // "r.editor.tabSize": 4,
    // "r.linting.multipleDots": false,
    // "r.linting.pascalCase": false,

    "window.title": "${activeEditorLong}",
    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Alice",
    "scm.providers.visible": 0,
    "git.countBadge": "tracked",
    "git.detectSubmodules": false,
    "git.openDiffOnClick": false,
    "git.showInlineOpenFileAction": false,
    "git.showProgress": false,
    "editor.copyWithSyntaxHighlighting": false,
    "workbench.statusBar.visible": true,
    "code-runner.clearPreviousOutput": true,
    "terminal.external.osxExec": "iTerm.app",
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.enabled": true,
    "code-runner.enableAppInsights": false,
    "workbench.settings.useSplitJSON": true,
    "python.venvPath": "~/.pyenv",
    "AREPL.telemetry": false,
    "zenMode.fullScreen": false,
    "zenMode.hideLineNumbers": false,
    "python.dataScience.allowLiveShare": false,
    "python.dataScience.enabled": false,
    "python.dataScience.searchForJupyter": false,
    "python.linting.pylintCategorySeverity.convention": "Warning",
    "python.pythonPath": "/Users/riley/.pyenv/shims/python",
    "files.trimTrailingWhitespace": true,
    "editor.fontLigatures": true,
    "editor.lineHeight": 17,
    "editor.accessibilitySupport": "off",
    "code-runner.saveFileBeforeRun": true,
    "python.terminal.activateEnvironment": false,
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.fontFamily": "FuraCodeNerdFontComplete-Retina, Menlo, Monaco, 'Courier New', monospace",
    "files.defaultLanguage": "shellscript",
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "explorer.confirmDragAndDrop": false,
    "window.restoreWindows": "none",
    "markdownlint.run": "onSave",
    "workbench.editor.restoreViewState": false,
    "files.hotExit": "off",
    "workbench.editor.swipeToNavigate": true,
    "files.watcherExclude": {
        "**/.brew": true,
        "**/.git": true,
        "**/.vim/plugged": true
    },
    "search.exclude": {
        "**/.brew": true,
        "**/.git": true,
        "**/.vim/plugged": true
    },
    "files.exclude": {
        "**/.Rproj.user": true
    },
    "editor.matchBrackets": false,
    "bracket-pair-colorizer-2.colors": [
        "#FFFFFF",
        "#DEE0DF",
        "#C4C4C4"
    ],
    "bracket-pair-colorizer-2.excludedLanguages": [
        "python"
    ],
    "javascript.format.insertSpaceAfterSemicolonInForStatements": false,
    "javascript.validate.enable": false,
    "workbench.iconTheme": "eq-material-theme-icons-darker",
    "explorer.sortOrder": "filesFirst",
    "editor.fontFamily": "FiraCode-Retina, Menlo, Monaco, 'Courier New', monospace",
    "r.lsp.diagnostics": false,
    "problems.autoReveal": false,
    "problems.decorations.enabled": false,
    "outline.problems.enabled": false,
    "extensions.autoCheckUpdates": false,
    "git.detectSubmodulesLimit": 2,
    "git.autorefresh": false,
    "git.autoRepositoryDetection": false,
    "git.enabled": false
}